                                    <div class="card complementary_slide complementary-slide--{{ product.id }}">
                                      <div class="media">
                                        {%- if product.selected_or_first_available_variant.image != blank -%}
                                          {%- assign img = product.selected_or_first_available_variant.image -%}
                                        {%- else -%}
                                          {%- assign img = product.featured_image -%}
                                        {%- endif -%}
                                        {%- if media_size == "adapt" -%}
                                          {%- assign current_image = img | img_url: '380x' -%}
                                        {%- elsif media_size == "square" -%}
                                          {%- assign current_image = img | image_url: width: 380, height: 380, crop: 'center' -%}
                                        {%- else -%}
                                          {%- assign current_image = img | image_url: width: 380, height: 520, crop: 'center' -%}
                                        {%- endif -%}
                                        <img
                                          src="{{ current_image }}"
                                          alt="{{ product.featured_media.alt | escape }}"
                                          loading="lazy"
                                          width="{{ product.featured_media.width }}"
                                          height="{{ product.featured_media.height }}"
                                        >
                                      </div>
                                      <div class="complementary_slide_info">
                                        <a
                                          href="{{ product.url | default: '#' }}"
                                          class="is-inline-block card-details__link"
                                        >
                                          <span class="card-details__text h4">
                                            {{ product.title | escape }}
                                          </span>
                                          {% render 'price', product: product %}
                                        </a>
                                        {% if product.available %}
                                          <button
                                            type="button"
                                            tabindex="0"
                                            class="quick-add-btn quick_view_btn enabled button button--primary"
                                            role="button"
                                            data-product="{{product.handle}}"
                                          >
                                            {{ 'products.product.add_to_cart' | t }}
                                          </button>
                                        {% endif %}
                                      </div>
                                    </div>
                                  {%- endfor -%}
                                </div>
                              </div>
                            </aside>
                          {%- endif -%}
                          {{ 'section-product-recommendations.css' | asset_url | stylesheet_tag }}
                          {%- if block.settings.enable_quick_add -%}
