                          <div class="mt-4">
                            {{ block.settings.text }}
                          </div>
                        {%- endif -%}
                      {%- when 'richtext' -%}
                        {%- if block.settings.richtext != blank -%}
                          <div class="mt-4 rte"></div>
                            {{ block.settings.richtext }}
                          </div>
                        {%- endif -%}
                      {%- when 'description' -%}
                        {%- if product.description != blank -%}
                          <div class="product__description">
                            {{ product.description }}
                          </div>
                        {%- endif -%}
                      {%- when 'stock_counter' -%}
                        {%- assign low_stock_threshold = block.settings.low_stock_threshold -%}
                        {%- assign very_low_stock_threshold = block.settings.very_low_stock_threshold -%}
                        {%- assign initial_inventory = product.selected_or_first_available_variant.inventory_quantity -%}

                        {%- if product.selected_or_first_available_variant.available == false %}
                          {%- assign initial_stock_level = "sold-out" -%}
                          {%- assign initial_text_before = block.settings.sold_out_text -%}
                          {%- assign initial_text_after = "" -%}
                          {%- assign initial_show_number = false -%}
                        {%- elsif product.selected_or_first_available_variant.inventory_management != "shopify" -%}
                          {%- assign initial_stock_level = "available" -%}
                          {%- assign initial_text_before = block.settings.available_text -%}
                          {%- assign initial_text_after = "" -%}
                          {%- assign initial_show_number = false -%}
                        {%- elsif product.selected_or_first_available_variant.inventory_management == "shopify"
                          and product.selected_or_first_available_variant.inventory_quantity < 1 -%}
                          {%- assign initial_stock_level = "preorder" -%}
                          {%- assign initial_text_before = block.settings.preorder_text -%}
                          {%- assign initial_text_after = "" -%}
                          {%- assign initial_show_number = false -%}
                        {%- elsif initial_inventory == 1 %}
                          {%- assign initial_stock_level = "one" -%}
                          {%- assign initial_text_before = block.settings.one_left_text_before -%}
                          {%- assign initial_text_after = block.settings.one_left_text_after -%}
                          {%- assign initial_show_number = block.settings.one_show_number -%}
                        {%- elsif initial_inventory < very_low_stock_threshold %}
                          {%- assign initial_stock_level = "very-low" -%}
                          {%- assign initial_text_before = block.settings.very_low_text_before -%}
                          {%- assign initial_text_after = block.settings.very_low_text_after -%}
                          {%- assign initial_show_number = block.settings.very_low_show_number -%}
                        {%- elsif initial_inventory < low_stock_threshold %}
                          {%- assign initial_stock_level = "low" -%}
                          {%- assign initial_text_before = block.settings.low_text_before -%}
                          {%- assign initial_text_after = block.settings.low_text_after -%}
                          {%- assign initial_show_number = block.settings.low_show_number -%}
                        {%- else %}
                          {%- assign initial_stock_level = "high" -%}
                          {%- assign initial_text_before = block.settings.high_text_before -%}
                          {%- assign initial_text_after = block.settings.high_text_after -%}
                          {%- assign initial_show_number = block.settings.high_show_number -%}
                        {%- endif %}

                        {%- for variant in product.variants -%}
                          <div
                            style="display:none"
                            class="js-variant-inventory"
                            data-variant-id="{{ variant.id }}"
                            data-variant-inventory-management="{{ variant.inventory_management }}"
                            data-variant-inventory-qty="{{ variant.inventory_quantity }}"
                            data-variant-available="{{ variant.available }}"
                          ></div>
                        {%- endfor -%}

                        <style>
                          .stock-counter[data-stock-level="sold-out"] .stock-counter__text {
                            color: {{ block.settings.sold_out_text_color }};
                          }
                          [data-stock-level="available"].stock-counter .stock-counter__text {
                            color: {{ block.settings.available_text_color }};
                          }
                          [data-stock-level="preorder"].stock-counter .stock-counter__text {
                            color: {{ block.settings.preorder_text_color }};
                          }
                          [data-stock-level="high"].stock-counter .stock-counter__text {
                            color: {{ block.settings.high_text_color }};
                          }
                          [data-stock-level="high"].stock-counter .stock-counter__text > span {
                            color: {{ block.settings.high_number_color }};
                          }
                          [data-stock-level="low"].stock-counter .stock-counter__text {
                            color: {{ block.settings.low_text_color }};
                          }
                          [data-stock-level="low"].stock-counter .stock-counter__text > span {
                            color: {{ block.settings.low_number_color }};
                          }
                          [data-stock-level="very-low"].stock-counter .stock-counter__text {
                            color: {{ block.settings.very_low_text_color }};
                          }
                          [data-stock-level="very-low"].stock-counter .stock-counter__text > span {
                            color: {{ block.settings.very_low_number_color }};
                          }
                          [data-stock-level="one"].stock-counter .stock-counter__text {
                            color: {{ block.settings.one_left_text_color }};
                          }
                          [data-stock-level="one"].stock-counter .stock-counter__text > span {
                            color: {{ block.settings.one_left_number_color }};
                          }
                        </style>

                        <div
                          class="stock-counter js-stock-counter"
                          data-stock-level="{{ initial_stock_level }}"
                          data-low-stock-threshold="{{ low_stock_threshold }}"
                          data-very-low-stock-threshold="{{ very_low_stock_threshold }}"
                          data-inventory-management="{{ product.selected_or_first_available_variant.inventory_management }}"
                          data-high-stock-before-text="{{ block.settings.high_text_before }}"
                          data-high-stock-after-text="{{ block.settings.high_text_after }}"
                          data-high-stock-show-number="{{ block.settings.high_show_number }}"
                          data-low-stock-before-text="{{ block.settings.low_text_before }}"
                          data-low-stock-after-text="{{ block.settings.low_text_after }}"
                          data-low-stock-show-number="{{ block.settings.low_show_number }}"
                          data-very-low-stock-before-text="{{ block.settings.very_low_text_before }}"
                          data-very-low-stock-after-text="{{ block.settings.very_low_text_after }}"
                          data-very-low-show-number="{{ block.settings.very_low_show_number }}"
                          data-one-left-before-text="{{ block.settings.one_left_text_before }}"
                          data-one-left-after-text="{{ block.settings.one_left_text_after }}"
                          data-one-left-show-number="{{ block.settings.one_left_show_number }}"
                          data-sold-out-text="{{ block.settings.sold_out_text }}"
                          data-available-text="{{ block.settings.available_text }}"
                          data-preorder-text="{{ block.settings.preorder_text }}"
                        >
                          <p class="stock-counter__text js-stock-counter-text">
                            {{ initial_text_before }}
                            {% if initial_show_number == true %}
