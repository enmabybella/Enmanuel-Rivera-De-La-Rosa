{{ 'section-text-columns-with-images.css' | asset_url | stylesheet_tag }}

{% style %}
  #{{ section.id }} p, #{{ section.id }} h3 {
      color: {{section.settings.color_text}};
  }
  #{{ section.id }} .placeholder-svg path{
    fill: {{section.settings.color_text}};
  }
  #{{ section.id }} .icon {
    background-color: {{section.settings.icon_background}};
    min-width: {{section.settings.icons_circle_size}}px;
    width: {{section.settings.icons_circle_size}}px;
    height: {{section.settings.icons_circle_size}}px;
    padding: {{section.settings.icons_circle_padding}}px;
  }
  #shopify-section-{{ section.id }} .section {
    padding-top: {{ section.settings.padding-top }}px;
    padding-bottom: {{ section.settings.padding-bottom }}px;
  }
  #{{ section.id }} .title {
    color: {{section.settings.color_text}};
  }
  #{{ section.id }} .icon path {
    fill: {{settings.colors_accent_1}};
  }
{% endstyle %}

<div
  id="{{ section.id }}"
  class="text-columns-with-images-section section color-{{ section.settings.color_bg }} content_alignment--{{ section.settings.content_alignment }} {% if section.settings.enable_section_borders %}section-with--border{% endif %}"
>
  <div class="page-width">
    {% if section.settings.heading != blank %}
      <h2 class="title data-aos data-aos--fade-left data-aos--delay-200">
        {{ section.settings.heading | escape }}
      </h2>
    {% endif %}

    <div class="columns  is-marginless horizontal-column ">
      {%- for block in section.blocks -%}
        <div class="section-grid column is-4-desktop is-6-tablet is-12-mobile {% if section.settings.icon_postion == 'top' %}icon-position-top{% endif %} data-aos data-aos--fade-up data-aos--delay-200">
          {% if block.settings.image != blank %}
            <img
              src="{{ block.settings.image | img_url: '720x' }}"
              srcset="
                {%- if block.settings.image.width >= 200 -%}{{ block.settings.image | img_url: '200x' }} 200w,{%- endif -%}
                {%- if block.settings.image.width >= 320 -%}{{ block.settings.image | img_url: '320x' }} 320w,{%- endif -%}
                {%- if block.settings.image.width >= 480 -%}{{ block.settings.image | img_url: '480x' }} 480w,{%- endif -%}
                {%- if block.settings.image.width >= 768 -%}{{ block.settings.image | img_url: '768x' }} 768w,{%- endif -%}
                {%- if block.settings.image.width >= 1024 -%}{{ block.settings.image | img_url: '1024x' }} 1024w,{%- endif -%}
                {%- if block.settings.image.width >= 1400 -%}{{ block.settings.image | img_url: '1400x' }} 1400w,{%- endif -%}
                {%- if block.settings.image.width >= 1920 -%}{{ block.settings.image | img_url: '1920x' }} 1920w{%- endif -%}
              "
              sizes="(min-width: 1100px){% if section.blocks.size > 1 %}700px{% else %}940px{% endif %}, (min-width: 750px) {% if section.blocks.size > 1 %}calc((100vw - 10rem) / 2){% else %}calc(100vw - 10rem){% endif %}, calc(100vw - 3rem)"
              height="{{ block.settings.image.height }}"
              width="{{ block.settings.image.width }}"
              alt="{{ block.settings.image.alt | escape }}"
              loading="lazy"
              class="icon"
              style="max-width: 25px; object-position:{{block.settings.image.presentation.focal_point}}; object-fit: cover;"
            >
          {% elsif block.settings.choose_icon == 'styling' %}
            <div class="icon">
              {% render 'icon-styling' %}
            </div>
          {% elsif block.settings.choose_icon == 'shipping' %}
            <div class="icon">
              {% render 'icon-shipping' %}
            </div>
          {%- elsif block.settings.choose_icon == 'payment' -%}
            <div class="icon">
              {% render 'icon-payment' %}
            </div>
          {%- elsif block.settings.choose_icon == 'support' -%}
            <div class="icon">
              {% render 'icon-support' %}
            </div>
          {%- elsif block.settings.choose_icon == 'cart' -%}
            <div class="icon">
              {% render 'icon-cart' %}
            </div>
          {%- elsif block.settings.choose_icon == 'discount' -%}
            <div class="icon">
              {% render 'icon-discount-title' %}
            </div>
          {%- elsif block.settings.choose_icon == 'ecommerce' -%}
            <div class="icon">
              {% render 'icon-ecommerce' %}
            </div>
          {%- elsif block.settings.choose_icon == 'gift' -%}
            <div class="icon">
              {% render 'icon-gift' %}
            </div>
          {%- elsif block.settings.choose_icon == 'offer' -%}
            <div class="icon">
              {% render 'icon-offer' %}
            </div>
          {%- elsif block.settings.choose_icon == 'price' -%}
            <div class="icon">
              {% render 'icon-price' %}
            </div>
          {%- elsif block.settings.choose_icon == 'retail' -%}
            <div class="icon">
              {% render 'icon-retail' %}
            </div>
          {%- elsif block.settings.choose_icon == 'card' -%}
            <div class="icon">
              {% render 'icon-card' %}
            </div>
          {%- elsif block.settings.choose_icon == '24_7' -%}
            <div class="icon">
              {% render 'icon-24_7' %}
            </div>
          {%- elsif block.settings.choose_icon == 'savings' -%}
            <div class="icon">
              {% render 'icon-savings' %}
            </div>
          {%- elsif block.settings.choose_icon == 'chat' -%}
            <div class="icon">
              {% render 'icon-chat' %}
            </div>
          {%- elsif block.settings.choose_icon == 'quality' -%}
            <div class="icon">
              {% render 'icon-quality' %}
            </div>
          {%- elsif block.settings.choose_icon == 'best_choice' -%}
            <div class="icon">
              {% render 'icon-best_choice' %}
            </div>
          {%- elsif block.settings.choose_icon == 'faq' -%}
            <div class="icon">
              {% render 'icon-faq' %}
            </div>
          {%- elsif block.settings.choose_icon == 'email_support' -%}
            <div class="icon">
              {% render 'icon-email_support' %}
            </div>
          {%- elsif block.settings.choose_icon == 'business_hours' -%}
            <div class="icon">
              {% render 'icon-business_hours' %}
            </div>
          {%- elsif block.settings.choose_icon == 'bestseller' -%}
            <div class="icon">
              {% render 'icon-bestseller' %}
            </div>
          {% elsif block.settings.icon != blank -%}
            <div class="icon">
              <span>{{ block.settings.icon }}</span>
            </div>
          {%- endif -%}

          <div class="mt-3 text-columns-contents {% if section.settings.columns_same_height %}columns_same_height{% endif %}">
            {%- if block.settings.block_heading != blank -%}
              <h3 class="title">{{ block.settings.block_heading }}</h3>
            {%- endif -%}
            {%- if block.settings.text != blank -%}
              <div class="text data-aos data-aos--fade-in rte">
                {{ block.settings.text }}
              </div>
            {%- endif -%}
            {%- if block.settings.button_label != blank -%}
              <div class="mt-5">
                <a
                  href="{{ block.settings.button_link }}"
                  class="button button--primary data-aos data-aos--fade-in"
                  {% if block.settings.button_link == blank %}
                    aria-disabled="true"
                  {% endif %}
                >
                  {{- block.settings.button_label | escape -}}
                </a>
              </div>
            {%- endif -%}
          </div>
        </div>
      {%- endfor -%}
    </div>
  </div>
</div>

